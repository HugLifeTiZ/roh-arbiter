                          <widget class="GtkImage" id="saveLogImage">
                            <property name="visible">True</property>
                            <property name="stock">gtk-save-as</property>
                            <property name="icon-size">1</property>
                          </widget>
                        </child>
                      </widget>
                    </child>
                    <child>
