                                <signal name="toggled" handler="SetTabPosition"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkRadioMenuItem" id="rightTabMenuItem">
                                <property name="visible">True</property>
                                <property name="label">_Right</property>
                                <property name="use_underline">True</property>
                                <property name="draw_as_radio">True</property>
                                <property name="group">leftVertTabMenuItem</property>
                                <signal name="toggled" handler="SetTabPosition"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkRadioMenuItem" id="topTabMenuItem">
                                <property name="visible">True</property>
                                <property name="label">_Top</property>
                                <property name="use_underline">True</property>
                                <property name="draw_as_radio">True</property>
                                <property name="group">leftVertTabMenuItem</property>
                                <signal name="toggled" handler="SetTabPosition"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkRadioMenuItem" id="bottomTabMenuItem">
                                <property name="visible">True</property>
                                <property name="label">_Bottom</property>
                                <property name="use_underline">True</property>
                                <property name="draw_as_radio">True</property>
                                <property name="group">leftVertTabMenuItem</property>
                                <signal name="toggled" handler="SetTabPosition"/>
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkMenuItem" id="compactModeMenuItem">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">_Compact Mode</property>
                        <property name="use_underline">True</property>
                        <child>
                          <widget class="GtkMenu" id="menu1">
                            <property name="visible">True</property>
                            <child>
                              <widget class="GtkCheckMenuItem" id="hideIconsMenuItem">
                                <property name="visible">True</property>
                                <property name="has_tooltip">True</property>
                                <property name="tooltip">Hides the sport icons on each tab.</property>
                                <property name="label">Hide _Sport Icons</property>
                                <property name="use_underline">True</property>
                                <signal name="toggled" handler="ToggleHideIcons"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkCheckMenuItem" id="hideCloseMenuItem">
                                <property name="visible">True</property>
                                <property name="has_tooltip">True</property>
                                <property name="tooltip">Hides the close buttons on each tab. The tabs can still be closed with the menu or the Escape key.</property>
                                <property name="label">Hide _Close Button</property>
                                <property name="use_underline">True</property>
                                <signal name="toggled" handler="ToggleHideClose"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkCheckMenuItem" id="hideButtonsMenuItem">
                                <property name="visible">True</property>
                                <property name="has_tooltip">True</property>
                                <property name="tooltip">Hides the Resolve and Undo buttons on the duel tab. The menu can be used, or Ctrl+R and Ctrl+Z.</property>
