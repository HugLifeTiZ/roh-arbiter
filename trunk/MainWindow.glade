                        <property name="label">_Save Report As...</property>
                        <property name="visible">True</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip">Saves the shift report to a file of your choice.</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">False</property>
                        <signal name="activate" handler="SaveShiftReport"/>
                        <child internal-child="image">
                          <widget class="GtkImage" id="saveReportImage">
                            <property name="visible">True</property>
                            <property name="stock">gtk-save-as</property>
                            <property name="icon-size">1</property>
                          </widget>
                        </child>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkImageMenuItem" id="quitMenuItem">
                        <property name="label" translatable="yes">_Quit</property>
                        <property name="visible">True</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip">Press N for guts and glory, press Y for work and worry.</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">False</property>
                        <signal name="activate" handler="QuitArbiter"/>
