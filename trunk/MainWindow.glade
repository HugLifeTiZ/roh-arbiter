<?xml version="1.0"?>
<glade-interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy project-wide -->
  <widget class="GtkWindow" id="mainWin">
    <property name="title">Arbiter</property>
    <property name="window_position">center</property>
    <signal name="size_allocate" handler="SaveSize"/>
    <signal name="delete_event" handler="QuitArbiter"/>
    <child>
      <widget class="GtkVBox" id="mainWidget">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <widget class="GtkMenuBar" id="menubar">
            <property name="visible">True</property>
            <child>
              <widget class="GtkMenuItem" id="shiftMenuItem">
                <property name="visible">True</property>
                <property name="label">_Shift</property>
                <property name="use_underline">True</property>
                <child>
                  <widget class="GtkMenu" id="shiftMenu">
                    <property name="visible">True</property>
                    <child>
                      <widget class="GtkCheckMenuItem" id="fightNightCheckMenuItem">
                        <property name="visible">True</property>
                        <property name="tooltip">If checked, the shift report will be grouped by sport, and the sport will be included in the duel logs.</property>
                        <property name="label" translatable="yes">_Fight Night</property>
                        <property name="use_underline">True</property>
                        <signal name="toggled" handler="FightNightToggled"/>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkMenuItem" id="brawlMenuItem">
                        <property name="visible">True</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip" translatable="yes">Starts a brawl.</property>
                        <property name="label" translatable="yes">_Brawl...</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="StartBrawl"/>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkSeparatorMenuItem" id="separatormenuitem1">
                        <property name="visible">True</property>
                      </widget>
                    </child>
                    <child>
