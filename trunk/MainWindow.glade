                        <property name="visible">True</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip">Disables automatic ending of the duel, and always enables the resolver.</property>
                        <property name="label">_Manual Mode</property>
                        <property name="use_underline">True</property>
                        <signal name="toggled" handler="ToggleManual"/>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkSeparatorMenuItem" id="duelMenuSeparator2">
                        <property name="visible">True</property>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkImageMenuItem" id="saveLogMenuItem">
                        <property name="label">_Save Log As...</property>
                        <property name="visible">True</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip">Save the duel log to a file of your choice.</property>
                        <property name="use_underline">True</property>
                        <property name="use_stock">False</property>
                        <signal name="activate" handler="SaveDuelLog"/>
