                                <property name="use_underline">True</property>
                                <signal name="toggled" handler="ToggleHideButtons"/>
                              </widget>
                            </child>
                            <child>
                              <widget class="GtkCheckMenuItem" id="smallScoreMenuItem">
                                <property name="visible">True</property>
                                <property name="has_tooltip">True</property>
                                <property name="tooltip">If enabled, the scoreboard will be displayed with a smaller font size.</property>
                                <property name="label">S_mall Scoreboard</property>
                                <property name="use_underline">True</property>
                                <signal name="toggled" handler="ToggleSmallScore"/>
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>
          </widget>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
