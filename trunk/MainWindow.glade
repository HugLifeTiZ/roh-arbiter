                          <widget class="GtkImage" id="quitImage">
                            <property name="visible">True</property>
                            <property name="stock">gtk-quit</property>
                            <property name="icon-size">1</property>
                          </widget>
                        </child>
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>
            <child>
